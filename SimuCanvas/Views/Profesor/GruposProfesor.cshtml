@{
    ViewData["Title"] = "GruposProfesor";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<title>Cursos Profesor</title>
<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
<link rel="stylesheet" href="~/css/GruposProfesorSheet.css">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css">

<div class="container">
    <div class="row">
        <div class="col-md-3 left-container">
            <img src="~/Images/ulacitlogo.png" class="img-fluid mb-5" alt="ULACIT LOGO">
            <div class="home-btn">
                <a href="@Url.Action("Profesor", "Home")" type="button" class="btn btn-secondary btn-block mb-2">
                    <i class="fas fa-home"></i>
                    Home
                </a>
                <div class="perfil-btn">
                    <a href="@Url.Action("PerfilProfesor", "Profesor")" type="button" class="btn btn-secondary btn-block">
                        <i class="fas fa-user"></i>
                        Perfil
                    </a>
                </div>
                <div class="signout-btn">
                    <form asp-action="SignOut" asp-controller="Profesor" method="post">
                        <button type="submit" class="btn btn-dark btn-block">
                            <i class="fas fa-sign-out-alt"></i>
                            Sign Out
                        </button>
                    </form>
                </div>
            </div>
        </div>
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GruposProfesor</title>
</head>
<body>
    <h1>groups</h1>
</body>
</html>
